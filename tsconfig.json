{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
   // "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",

    // "noErrorTruncation": true,
    // "noImplicitReturns": false,
    // "noUnusedLocals": false,
    // "noUnusedParameters": false,
    "strict": true,
    //"strictFunctionTypes": false,
  },
  "include": [
    "next-env.d.ts",
    "*.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/cities/[city]", "pages/meetups/index.spec.js", "pages/meetups/index.spec.js", "pages/App.test.js"  ],
  "exclude": [
    "node_modules"
  ]
}
